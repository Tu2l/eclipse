package com.cg.app;

public class SBU {
	private int sbuId;
	private String name;
	private String head;

	public SBU() {
	}

	public SBU(String city, String state, int pin) {
		super();
		this.name = city;
		head = state;
		this.sbuId = pin;
	}

	public String getCity() {
		return name;
	}

	public void setCity(String city) {
		this.name = city;
	}

	public String getState() {
		return head;
	}

	public void setState(String state) {
		head = state;
	}

	public int getPin() {
		return sbuId;
	}

	public void setPin(int pin) {
		this.sbuId = pin;
	}

	@Override
	public String toString() {
		return "Address [city=" + name + ", State=" + head + ", pin=" + sbuId + "]";
	}

}
