package com.cg.app;

public class SBU {
	private String city;
	private String State;
	private int id;

	public SBU() {
	}

	public SBU(String city, String state, int pin) {
		super();
		this.city = city;
		State = state;
		this.id = pin;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getState() {
		return State;
	}

	public void setState(String state) {
		State = state;
	}

	public int getPin() {
		return id;
	}

	public void setPin(int pin) {
		this.id = pin;
	}

	@Override
	public String toString() {
		return "Address [city=" + city + ", State=" + State + ", pin=" + id + "]";
	}

}
